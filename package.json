{
    "name": "network-cyber-sim-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "client": "npm run dev -w client",
        "server": "npm run dev -w server",
        "start": "concurrently \"npm run server\" \"npm run client\""
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}